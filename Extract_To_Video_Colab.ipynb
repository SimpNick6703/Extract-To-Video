    "def setup_chrome_driver():\n",
    "    \"\"\"Set up Chrome driver with optimal settings for canvas capture.\"\"\"\n",
    "    \n",
    "    chrome_options = Options()\n",
    "    \n",
    "    # Essential Chrome options for Colab environment\n",
    "    chrome_options.add_argument('--headless')\n",
    "    chrome_options.add_argument('--no-sandbox')\n",
    "    chrome_options.add_argument('--disable-dev-shm-usage')\n",
    "    chrome_options.add_argument('--disable-gpu')\n",
    "    chrome_options.add_argument('--disable-features=VizDisplayCompositor')\n",
    "    chrome_options.add_argument('--disable-web-security')\n",
    "    chrome_options.add_argument('--allow-running-insecure-content')\n",
    "    chrome_options.add_argument('--disable-blink-features=AutomationControlled')\n",
    "    \n",
    "    # Set window size for high resolution capture\n",
    "    chrome_options.add_argument(f'--window-size={CONFIG[\"viewport_width\"]},{CONFIG[\"viewport_height\"]}')\n",
    "    \n",
    "    # Performance optimizations\n",
    "    chrome_options.add_argument('--disable-extensions')\n",
    "    chrome_options.add_argument('--disable-plugins')\n",
    "    chrome_options.add_argument('--disable-images')  # We only need canvas content\n",
    "    \n",
    "    # User agent to avoid detection\n",
    "    chrome_options.add_argument('--user-agent=Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/91.0.4472.124 Safari/537.36')\n",
    "    \n",
    "    try:\n",
    "        driver = webdriver.Chrome(options=chrome_options)\n",
    "        \n",
    "        # Set timeouts\n",
    "        driver.implicitly_wait(10)\n",
    "        driver.set_page_load_timeout(60)\n",
    "        \n",
    "        print(\"‚úÖ Chrome driver setup successfully!\")\n",
    "        return driver\n",
    "        \n",
    "    except Exception as e:\n",
    "        print(f\"‚ùå Failed to setup Chrome driver: {e}\")\n",
    "        return None\n",
    "\n",
    "# Test driver setup\n",
    "print(\"üîß Testing Chrome driver setup...\")\n",
    "test_driver = setup_chrome_driver()\n",
    "if test_driver:\n",
    "    test_driver.quit()\n",
    "    print(\"‚úÖ Chrome driver test successful!\")"
   ]
